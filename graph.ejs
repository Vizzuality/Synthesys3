<!DOCTYPE html>
<html>
<head>
  <!-- set the encoding of your site -->
  <meta charset="utf-8">
  <!-- set the viewport width and initial-scale on mobile devices -->
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Synthesys</title>
  <!-- include the site stylesheet -->
  <link media="all" rel="stylesheet" href="css/main.css">
  <link
      href="https://fonts.googleapis.com/css?family=Roboto:100,100i,300,300i,400,400i,500,500i,700,700i,900,900i"
      rel="stylesheet">
  <!-- include lodash -->
  <script src="https://cdn.jsdelivr.net/npm/lodash@4.17.4/lodash.min.js"></script>
  <!-- include json2csv -->
  <script src="js/csv-converter.js"></script>
  <!-- include jQuery library -->
  <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.3/jquery.min.js" defer></script>
  <script>window.jQuery || document.write('<script src="js/jquery-1.11.3.min.js" defer><\/script>')</script>
  <!-- include JavaScript for maps/graphs -->
  <script src="https://code.highcharts.com/highcharts.js" defer></script>
  <script src="https://code.highcharts.com/maps/modules/map.js" defer></script>
  <script src="https://code.highcharts.com/modules/treemap.js" defer></script>
  <script src="//d3js.org/d3.v3.min.js"></script>
  <script src="//d3js.org/d3.geo.projection.v0.min.js"></script>
  <script src="//d3js.org/topojson.v1.min.js"></script>

  <!-- include custom JavaScript -->
  <script src="js/constants.js" defer></script>
  <script src="js/sql-queries.js" defer></script>
  <script src="js/app.js" defer></script>
  <script src="js/map-component.js" defer></script>
  <script src="js/main.js" defer></script>
  <script src="js/lib.js" defer></script>
  <script type="application/javascript">
    window._env = '<%= environment %>';
  </script>
</head>
<body>
<!-- main container of all the page elements -->
<div id="wrapper">
  <!-- header of the page -->
  <header id="header">
    <div class="container">
      <!-- logo of the page -->
      <div class="logo">
        <a href="/index.html">
          <img src="images/logo.jpg" height="52" width="200"
               alt="synthesys synthesys of systematic resources">
        </a>
      </div>
      <!-- main navigation area -->
      <div class="header-content">
        <a href="/index.html" class="nav-opener"><span></span></a>
        <div class="nav-area">
          <nav id="nav">
            <ul>
              <li class="active">
                <a href="/graph.html">Overview</a>
              </li>
              <li>
                <a href="/showcases">Showcases</a>
              </li>
              <li>
                <a href="/about.html">About</a>
              </li>
            </ul>
          </nav>
        </div>
      </div>
    </div>
  </header>
  <!-- contain main informative part of the site -->
  <main id="main" role="main">
    <!-- filter area -->
    <div class="filter-area">
      <div class="container">
        <a href="#" class="filter-by"><span class="open">Filter by</span><span class="close">Overview</span></a>
        <div class="filter-holder">
          <ul class="filter-list">
            <li><a href="#" class="all js-filter-restore"><span>All</span></a></li>
            <li>
              <div class="autocomplete">
                <a href="#" class="opener"><span class="js-country-filter-label">Select a Country</span></a>
                <div class="drop">
                  <div class="input">
                    <input class="js-country-search" type="search" placeholder="Search...">
                  </div>
                  <ul class="list js-filter" data-filter-type="country">
                    <li id="data-filter-placeholder" data-filter-type="country" data-value=""><a>Select a Country</a></li>
                  </ul>
                </div>
              </div>
            </li>
            <li>
              <select class="js-filter" data-filter-type="discipline">
                <option value="">Select a Discipline</option>
              </select>
            </li>
            <li>
              <select class="js-filter" data-filter-type="funding_round">
                <option value="">Select a Founding Round</option>
              </select>
            </li>
          </ul>
        </div>
      </div>
    </div>
    <!-- filter info -->
    <div class="filter-info container">
      <div class="text">
        <p class="js-dynamic-sentence"></p>
      </div>
      <div class="charts-block">
        <div class="col">
          <div class="col-holder">
            <div class="chart-heading">
              <h2 class="chart-title">Researchers by Gender</h2>
              <h3 class="chart-sub-title js-country-filter-dependent"></h3>
            </div>
            <div class="gender-donut-chart"></div>
          </div>
        </div>
        <div class="col">
          <div class="col-holder">
            <div class="chart-heading">
              <h2 class="chart-title">Researchers by Time</h2>
              <h3 class="chart-sub-title js-country-filter-dependent"></h3>
            </div>
            <div class="line-chart"></div>
          </div>
        </div>
        <div class="col">
          <div class="col-holder">
            <div class="chart-heading">
              <h2 class="chart-title">Researchers by Region</h2>
              <h3 class="chart-sub-title js-country-filter-dependent"></h3>
              <div class="chart-info">
                <span class="js-discipline-filter-dependent"></span>
              </div>
            </div>
            <div class="map-chart"></div>
          </div>
        </div>
        <div class="col">
          <div class="col-holder">
            <div class="chart-heading">
              <h2 class="chart-title">Researchers by Discipline</h2>
              <h3 class="chart-sub-title js-country-filter-dependent"></h3>
            </div>
            <div class="bubbles-chart-holder">
              <div class="bubbles-chart" data-src="inc/bubbles-chart-data.json"></div>
            </div>
          </div>
        </div>
        <div class="col">
          <div class="col-holder">
            <div class="chart-heading">
              <h2 class="chart-title">Papers per Year</h2>
              <h3 class="chart-sub-title js-country-filter-dependent"></h3>
            </div>
            <div class="column-chart"></div>
          </div>
        </div>
        <div class="col">
          <div class="col-holder">
            <div class="chart-heading">
              <h2 class="chart-title">Researcher Type</h2>
              <h3 class="chart-sub-title js-country-filter-dependent"></h3>
            </div>
            <div class="researcher-type-donut-chart"></div>
          </div>
        </div>
        <div class="col">
          <div class="col-holder">
            <div class="chart-heading">
              <h2 class="chart-title">What Collections did the Researchers Access?</h2>
              <h3 class="chart-sub-title js-country-filter-dependent"></h3>
            </div>
            <div class="treemap-chart"></div>
          </div>
        </div>
        <div class="col lg">
          <div class="col-holder">
            <div class="chart-heading">
              <h2 class="chart-title">Researcher Movement by Region</h2>
              <h3 class="chart-sub-title js-country-filter-dependent"></h3>
            </div>
            <div class="tree-chart"></div>
          </div>
        </div>
      </div>
    </div>
    <!-- research box -->
    <div class="research-box">
      <a href="#" class="research-opener"><span>Research Papers</span></a>
      <div class="research-slide">
        <div class="slide-holder">
          <div class="container">
            <div class="btn-holder">
              <a href="#" class="download js-table-download">Download Data</a>
            </div>
            <div class="table-holder">
              <table>
                <thead>
                <tr>
                  <th class="col1">
                    <span>Year</span>
                    <button class="sort js-table-sort" data-orderby="year">
                      <i class="icon-arrow-combo"></i>
                    </button>
                  </th>
                  <th class="col2">
                    <span>Title</span>
                    <button class="sort js-table-sort" data-orderby="title">
                      <i class="icon-arrow-combo"></i>
                    </button>
                  </th>
                  <th class="col3">
                    <span>Publisher</span>
                    <button class="sort js-table-sort" data-orderby="publisher">
                      <i class="icon-arrow-combo"></i>
                    </button>
                  </th>
                  <th class="col4">
                    <span>Author</span>
                    <button class="sort js-table-sort" data-orderby="authors">
                      <i class="icon-arrow-combo"></i>
                    </button>
                  </th>
                  <th class="col5"><span>Vol.</span></th>
                  <th class="col6"><span>PP.</span></th>
                </tr>
                </thead>
                <tbody class="js-table-body"></tbody>
              </table>
            </div>
            <!-- paging area -->
            <div class="paging-area">
              <a href="#" class="js-pagination-prev prev btn-default">Prev</a>
              <ul class="paging js-table-pagination"></ul>
              <a href="#" class="js-pagination-next next btn-default">Next</a>
            </div>
          </div>
        </div>
      </div>
    </div>
  </main>
</div>
</body>
</html>
